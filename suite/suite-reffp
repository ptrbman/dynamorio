## Suite for SPEC2000 on ref dataset -- just fp benchmarks!
BMARKBASE/benchmarks/spec2000/fp/mesa	(make COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/art	(make COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/equake	(make COPYDATA=yes REPLACEHERE run_ref)
#-SPLIT
BMARKBASE/benchmarks/spec2000/fp/apsi	(make REF_MINS=120 COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/swim	(make REF_MINS=150 COPYDATA=yes REPLACEHERE run_ref)
#-SPLIT
BMARKBASE/benchmarks/spec2000/fp/ammp	(make COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/sixtrack	(make COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/mgrid	(make REF_MINS=200 COPYDATA=yes REPLACEHERE run_ref)
#-SPLIT
BMARKBASE/benchmarks/spec2000/fp/wupwise	(make REF_MINS=300 COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/applu	(make REF_MINS=200 COPYDATA=yes REPLACEHERE run_ref)

# FORTRAN 90
BMARKBASE/benchmarks/spec2000/fp/facerec	(make REF_MINS=300 COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/fma3d	(make REF_MINS=300 COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/galgel	(make REF_MINS=300 COPYDATA=yes REPLACEHERE run_ref)
BMARKBASE/benchmarks/spec2000/fp/lucas	(make REF_MINS=300 COPYDATA=yes REPLACEHERE run_ref)
